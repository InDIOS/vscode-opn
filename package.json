{
  "name": "vscode-opn",
  "displayName": "Open File in App",
  "description": "Integrates node.js module 'opn' to open files in the default application for the file type set in the OS",
  "version": "1.0.2",
  "publisher": "lfurzewaddock",
  "icon": "vscode-opn.png",
  "galleryBanner": {
    "color": "#A88037",
    "theme": "dark"
  },
  "keywords": [
    "open", 
    "launch", 
    "file",
    "program", 
    "app", 
    "application", 
    "default", 
    "html", 
    "htm", 
    "txt", 
    "json", 
    "xml", 
    "log"
  ],
  "bugs": {
    "url": "https://github.com/lfurzewaddock/vscode-opn/issues"
  },
  "homepage": "https://github.com/lfurzewaddock/vscode-opn/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/lfurzewaddock/vscode-opn"
  },
  "engines": {
    "vscode": "^0.10.6"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.opn",
        "title": "Open"
      }
    ],
    "keybindings": [
      {
        "command": "extension.opn",
        "key": "ctrl+alt+o",
        "mac": "cmd+alt+o"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "opn": "^4.0.0"
  },
  "devDependencies": {
    "jscs": "^2.10.1",
    "jshint": "^2.9.1",
    "vscode": "^0.11.0"
  },
  "license": "See LICENSE.txt"
}